<table>
  <% var i; for(i=0; i< quizes.length; i++) {%>
    <tr>
      <td>
        <a href="quizes/<%= quizes[i].id %>"><%= quizes[i].pregunta %></a>
      </td>
      <td>
        <a href="quizes/<%= quizes[i].id %>/edit">editar</a>
      </td>
      <td>
        <form action="quizes/<%= quizes[i].id %>/delete?_method=delete" method="post">
          <button type="submit" name="btnDelete" onClick="return confirm('Borrar: <%= quizes[i].pregunta %> ?')">borrar</button>
        </form>
      </td>
    </tr>
  <% } %>
</table>
<a href="/quizes/new"><button type="button" name="button">Crear pregunta</button></a>
